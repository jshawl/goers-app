<%= form_for [@artist, @image] do |f| %>

  <%= f.label :date %>
  <%= f.text_field :date %>

  <%= f.label :venue %>
  <%= f.text_field :venue %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :photo_url, "Photo URL" %>
  <%= f.text_field :photo_url %>

  <%= f.submit %>
<% end  %>
